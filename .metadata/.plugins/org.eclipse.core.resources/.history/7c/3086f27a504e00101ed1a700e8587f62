package arrays.in.sit;

public class ArrayDemo03 {
	public static void main(String[] args) {

		int arr[] = new int[4];

		arr[0] = 65;
		arr[1] = 48;
		arr[2] = 16;
		arr[3] = 325;

		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i] + "  ");
		}
		System.out.println("\nArray of Length:"+arr.length);
	}
}
